        <attr name="home_icon" format="reference" />
        <attr name="back_icon" format="reference" />
        <attr name="icon_padding" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="EmptyView">
        <attr name="loading_layout_img_an" format="integer|reference" />
